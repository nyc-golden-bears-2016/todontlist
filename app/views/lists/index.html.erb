<div class="text-container">

  <h1>Your To Don't Lists</h1>
  <% @user.lists.each do | list | %>
    <a href="lists/<%= list.id %>"><%= list.title %></a>
  <% end %>
</div>

<div class="text-container">

 <%= form_for :list, url: lists_path do |f| %>

      <div class="field">
        <%= f.label :list_title %>
        <br>
        <%= f.text_field :title, placeholder: "New lists title" %>
      </div>
      <%= f.submit "Create New List" %>

    <% end %>
</div>
